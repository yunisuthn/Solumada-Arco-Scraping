<%- include('partials/header') -%>
<!-- Loading div -->
<div class="loading" id="loding-div">
    <div>
        <h3>Scraping ... </h3>
    </div>
    <div class="loader" id="loadingAnimation"></div>
</div>

<!-- Table -->
<div class="container mt-5">
    <table class="table table-bordered" id="dataTable" width="100%" cellspacing="0">
        <thead>
            <tr>
                <th>Index</th>
                <th>Batch</th>
                <th>Client</th>
                <th>Priority</th>
                <th>Document</th>
                <th>Date</th>
            </tr>
        </thead>
            <tbody>
                <% rows.forEach((row)=> { %>
                    <tr>
                        <td><%= row.index %></td>
                        <td><%= row.batch %></td>
                        <td><%= row.client%></td>
                        <td><%= row.priority%></td>
                        <td><%= row.document%></td>
                        <td><%= row.date %></td>
                    </tr>
               <% }) %>
            </tbody>
        </table>
    </div>
<%- include('partials/footer') -%>